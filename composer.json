{
    "name": "nonz250/php-simple-ogp",
    "description": "This is a simple library for reading OGP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nozomi Hosaka",
            "email": "hosaka.non.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-mbstring": "*",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "SimpleOgp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "composer dump-autoload"
        ]
    }
}
